<% content_for :title, "Nearby" %>
<% content_for :googlemaps do %>
  <%= javascript_include_tag "http://maps.google.com/maps/api/js?sensor=true" %>
<% end %>
<div class="container">
  <div class="page-header">
    <h1><%= yield :title %></h1>
  </div>
  <div class="page-body row">
    <div class="content span7">
      <%= render partial: "layouts/media-grid-photos", locals: {photos: []} %>
    </div>
    <div class="dashboard span4-5 offset0-5">
      <div class="location">
        <div class="name">
          <h2>Nearby</h2>
        </div>
        <hr />
        <%= content_tag :div, nil, class: "map" %>
        <hr />
        <div class="nearby">
        </div>
      </div>
    </div>
  </div>
</div>
<%= render partial: "layouts/media-panel", locals: {photos: []} %>
